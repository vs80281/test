Public class Employee
{
private String name;
private int age;

public Employee(String name, int age){
super();
this.name=name;
this.age=age;
}
public String getName(){
return name;
}
public void setName(String name){
this.name=name
}
public int getAge(){
return age;
}
public void setAge(int age){
this.age=age;
}
public String toString()
{
return"Employee Name:"+name+"age:"+age;
}
}
public static List<Employee>createEmployeeList()
{
List<Employee>employeeList=new ArrayList<>();

Employee e1=new Employee("Tarun",211);
Employee e2=new Employee("Shyam",199);
Employee e3=new Employee("Ashok",31);
Employee e4=new Employee("Sohan",18);
Employee e5=new Employee("gary",226);

employeeList.add(e1);
employeeList.add(e2);
employeeList.add(e3);
employeeList.add(e4);
employeeList.add(e5);

return employeeList;
}
